 /dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>
#include "mt7981.dtsi"

/ {
	model = "ZX7981PM";
	compatible = "zx,zx7981pm", "mediatek,mt7981";

	aliases {
		led-boot = &led_4g_blue;
		led-failsafe = &led_4g_yellow;
		led-running = &led_wan;
		led-upgrade = &led_5g_blue;
		serial0 = &uart0;
	};

	chosen {
		bootargs = "console=ttyS0,115200n1 loglevel=8 earlycon=uart8250,mmio32,0x11002000";
		stdout-path = "serial0:115200n8";
	};

	memory {
		device_type = "memory";
		reg = <0x00 0x40000000 0x00 0x20000000>;
	};

	leds {
		compatible = "gpio-leds";

		led_5g_blue: led-5g-blue {
			function = LED_FUNCTION_WAN;
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&pio 12 GPIO_ACTIVE_LOW>;
		};

		led_5g_yellow: led-5g-yellow {
			function = LED_FUNCTION_INDICATOR;
			color = <LED_COLOR_ID_AMBER>;
			gpios = <&pio 11 GPIO_ACTIVE_LOW>;
		};

		led_wifi: led-wifi {
			function = LED_FUNCTION_WLAN;
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&pio 9 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		led_4g_blue: led-4g-blue {
			function = LED_FUNCTION_WAN;
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&pio 10 GPIO_ACTIVE_LOW>;
		};

		led_4g_yellow: led-4g-yellow {
			function = LED_FUNCTION_INDICATOR;
			color = <LED_COLOR_ID_AMBER>;
			gpios = <&pio 13 GPIO_ACTIVE_LOW>;
		};

		led_lan2: led-lan2 {
			label = "lan2";
			gpios = <&pio 4 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "netdev";
		};

		led_lan3: led-lan3 {
			label = "lan3";
			gpios = <&pio 22 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "netdev";
		};

		led_wan: led-wan {
			function = LED_FUNCTION_WAN;
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&pio 8 GPIO_ACTIVE_HIGH>;
		};
	};

	gpio-export {
		compatible = "gpio-export";
		#size-cells = <0>;

		5gpower {
			gpio-export,name = "5gpower";
			gpio-export,output = <1>;
			gpios = <&pio 25 GPIO_ACTIVE_LOW>;
		};

		fan {
			gpio-export,name = "fan";
			gpio-export,output = <0>;
			gpios = <&pio 5 GPIO_ACTIVE_LOW>;
		};

		5gpwrkey {
			gpio-export,name = "5gpwrkey";
			gpio-export,output = <1>;
			gpios = <&pio 2 GPIO_ACTIVE_LOW>;
		};

		5gsim {
			gpio-export,name = "5gsim";
			gpio-export,output = <0>;
			gpios = <&pio 15 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&pio 1 GPIO_ACTIVE_LOW>;
		};

		wps {
			label = "wps";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&pio 7 GPIO_ACTIVE_LOW>;
		};
	};
};

&eth {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;

	gmac0: mac@0 {
		compatible = "mediatek,eth-mac";
		reg = <0>;
		phy-mode = "2500base-x";

		fixed-link {
			speed = <2500>;
			full-duplex;
			pause;
		};
	};

	mdio: mdio-bus {
		#address-cells = <1>;
		#size-cells = <0>;

		switch@31 {
			compatible = "mediatek,mt7531";
			reg = <31>;
			reset-gpios = <&pio 39 GPIO_ACTIVE_HIGH>;
			interrupt-parent = <&pio>;
			interrupts = <38 IRQ_TYPE_LEVEL_HIGH>;

			ports {
				#address-cells = <1>;
				#size-cells = <0>;

				port@1 {
					reg = <1>;
					label = "lan2";
				};

				port@2 {
					reg = <2>;
					label = "lan3";
				};

				port@5 {
					reg = <5>;
					label = "wan";
					phy-mode = "2500base-x";
					fixed-link {
						speed = <2500>;
						full-duplex;
						pause;
					};
				};

				port@6 {
					reg = <6>;
					label = "cpu";
					ethernet = <&gmac0>;
					phy-mode = "2500base-x";
					fixed-link {
						speed = <2500>;
						full-duplex;
						pause;
					};
				};
			};
		};
	};
};

&spi0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&spi0_flash_pins>;

	spi_nand: flash@0 {
		compatible = "spi-nand";
		reg = <0>;
		spi-max-frequency = <52000000>;
		spi-tx-bus-width = <4>;
		spi-rx-bus-width = <4>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "BL2";
				reg = <0x000000 0x100000>;
			};

			partition@100000 {
				label = "u-boot-env";
				reg = <0x100000 0x80000>;
			};

			factory: partition@180000 {
				label = "Factory";
				reg = <0x180000 0x200000>;
			};

			partition@380000 {
				label = "FIP";
				reg = <0x380000 0x200000>;
			};

			partition@580000 {
				label = "woem";
				reg = <0x580000 0xa0000>;
			};

			partition@620000 {
				label = "ubi";
				reg = <0x620000 0x3800000>;
				compatible = "linux,ubi";
			};

			partition@3e20000 {
				label = "ubi2";
				reg = <0x3e20000 0x3800000>;
				compatible = "linux,ubi";
			};

			partition@7620000 {
				label = "wtinfo";
				reg = <0x7620000 0x60000>;
			};

			partition@7680000 {
				label = "nvram";
				reg = <0x7680000 0x60000>;
			};
		};
	};
};

&pio {
	spi0_flash_pins: spi0-pins {
		mux {
			function = "spi";
			groups = "spi0", "spi0_wp_hold";
		};

		conf-pu {
			pins = "SPI0_CS", "SPI0_HOLD", "SPI0_WP";
			drive-strength = <8>;
			bias-pull-up;
		};

		conf-pd {
			pins = "SPI0_CLK", "SPI0_MOSI", "SPI0_MISO";
			drive-strength = <8>;
			bias-pull-down;
		};
	};
};

&xhci {
	status = "okay";
	mediatek,u3p-dis-msk = <0x0>;
	phys = <&u2port0 PHY_TYPE_USB2>,
	       <&u3port0 PHY_TYPE_USB3>;
};

&wed {
	mediatek,dy_txbm = /bits/ 8 <0x1>;
	mediatek,txbm_budget = <8>;
	mediatek,txbm_init_sz = <8>;
	mediatek,txbm_max_sz = <32>;
	status = "okay";
};

&uart0 {
	status = "okay";
};

&watchdog {
	status = "okay";
};

&usb_phy {
	status = "okay";
};

&wifi {
	status = "okay";
};
